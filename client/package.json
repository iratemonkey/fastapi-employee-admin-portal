{
  "name": "react-client-boiler-plate",
  "version": "0.0.1",
  "private": true,
  "homepage": "https://react-boiler-plate.fake",
  "engines": {
    "node": "^10.13 || 12 || 14",
    "npm": ">=6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:watch": "jest --watch",
    "test:coverage": "jest --watch=false --coverage",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --watch --runInBand",
    "cy:install": "cypress install",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "start-server-and-test start:cli http://localhost:3000/list cy:open",
    "test:e2e:run": "start-server-and-test serve http://localhost:8811/list cy:run",
    "lint": "eslint . --cache-location node_modules/.cache/eslint",
    "format": "prettier --write \"**/*.+(js|json|css|md|mdx|html)\""
  },
  "dependencies": {
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@reach/dialog": "^0.11.2",
    "@reach/menu-button": "^0.11.2",
    "@reach/tabs": "^0.11.2",
    "@reach/tooltip": "^0.11.2",
    "@reach/visually-hidden": "^0.11.1",
    "bootstrap": "^4.5.3",
    "codegen.macro": "^4.0.0",
    "debounce-fn": "^4.0.0",
    "faker": "^5.1.0",
    "history": "^5.0.0",
    "match-sorter": "^5.0.0",
    "msw": "^0.21.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-error-boundary": "^3.0.2",
    "react-icons": "^3.11.0",
    "react-query": "^2.26.2",
    "react-query-devtools": "^2.6.3",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "react-scripts": "^4.0.0",
    "stop-runaway-react-effects": "^2.0.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^7.0.1",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.1.2",
    "@testing-library/react-hooks": "^3.4.2",
    "@testing-library/user-event": "^12.2.2",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "cross-env": "^7.0.2",
    "cypress": "^5.6.0",
    "cypress-hmr-restarter": "^1.4.1",
    "eslint-plugin-cypress": "^2.11.2",
    "husky": "^4.3.0",
    "inquirer": "^7.3.3",
    "is-ci": "^2.0.0",
    "is-ci-cli": "^2.1.2",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "react-test-renderer": "^17.0.1",
    "resolve": "^1.19.0",
    "serve": "^11.3.2",
    "start-server-and-test": "^1.11.5",
    "whatwg-fetch": "^3.5.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint && npm audit"
    }
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  },
  "description": "",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seantbaier/react-client-boiler-plate.git"
  },
  "keywords": [],
  "author": "Sean Baier @seantbaier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seantbaier/react-client-boilet-plate/issues"
  }
}
